<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp"
    android:background="@drawable/item_border"
    android:padding="5dp">

    <TextView
        android:id="@+id/tvTimeIn"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView26"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_toRightOf="@id/textView33"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User RattlerID: "
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tvUserId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView26"
        android:text="TextView"
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="23dp"
        android:layout_toRightOf="@id/tvUserId"
        android:text="Date Rented: "
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tvDate"
        android:layout_width="103dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView29"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView26"
        android:layout_marginTop="0dp"
        android:text="Cart Rented at: "
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tvTimeOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView26"
        android:layout_toRightOf="@id/textView31"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#0E0D0D"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView26"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="0dp"
        android:layout_toRightOf="@id/tvTimeOut"
        android:text="Cart Returned at: "
        android:textColor="#0E0D0D"
        android:textSize="16sp" />
</RelativeLayout>